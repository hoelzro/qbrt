
datatype Animal
  construct Dog
    dparam drool core/Bool
  end.

  construct Cat
    dparam hairball core/Bool
  end.
end.


func __main

lconstruct $0 ./Dog
lconstruct $0.0 core/True
lfunc $1 io/print

if $0.0 @NO_DROOL
const $1.0 "This dog drools.\n"
goto @END_DROOL

@NO_DROOL
const $1.0 "This dog has no drool.\n"

@END_DROOL
call \void $1

end.
